{
  "name": "bitcoin-luno-price",
  "productName": "Bitcoin(Luno) Price",
  "version": "1.0.0",
  "description": "A simple menubar/tray app for current price of bitcoin (luno)",
  "license": "Apache 2.0 License",
  "repository": "github.com/sicksand/bitcoin-luno-price",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "electron-fetch": "^1.0.0",
    "menubar": "^5.2.3"
  },
  "main": "main.js",
  "scripts": {
    "build-osx": "electron-packager blp-osx  bitcoin-luno-price --prune=true --platform=darwin --arch=x64 --overwrite --electronVersion=1.0.0 
--icon=assets/bitcoin-logo64.icns",
    "build-win32": "electron-packager blp-win32  bitcoin-luno-price --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/bitcoin-logo64.png 
--prune=true --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Bitcoin(Luno) Price\"",
    "start": "electron ."
  }
}
